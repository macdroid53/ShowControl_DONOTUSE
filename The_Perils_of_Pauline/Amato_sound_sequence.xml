<?xml version="1.0" encoding="utf-8"?>
<show_control>

  <!-- This sound sequence is common to many shows.  -->
  <sound_sequence>
    <version>1.0</version>

    <sequence_item>
      <name>Welcome-1</name>
      <type>operator_wait></type>
      <next>Welcome-2</next>
      <display></display>
      <function_key></function_key>
      <Q_number>1</Q_number>
      <macro_number></macro_number>
    </sequence_item>

   <sequence_item>
      <name>Welcome-2</name>
